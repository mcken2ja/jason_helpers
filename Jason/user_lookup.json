{
  "$jason": {
    "head": {
      "title": "User Lookup",
      "actions": {
        "$load": {
          "type": "$render"
        },
        "check_in": {
          "type": "$network.request",
          "options": {
            "url": "https://www.trufitportal.com/demo/api/manual_checkin.json?clsID={{$params.clsID}}&date={{$params.date}}&cuID={{$get.userid}}"
          },
          "success": {
            "type": "$back"
          }
        }
      },
      "styles": {
        "title": {
          "size": "25",
          "font": "HelveticaNeue-Bold"
        },
        "input_class": {
          "font": "HelveticaNeue",
          "size": "18"
        },
        "btn_add_class": {
          "align": "center",
          "height": "60",
          "font": "HelveticaNeue-Bold",
          "border_width": "1",
          "background": "#ffffff",
          "color": "#2377d9",
          "corner_radius": "5",
          "size": "18"
        },
        "btn_ready_class": {
          "align": "center",
          "height": "60",
          "font": "HelveticaNeue-Bold",
          "border_width": "1",
          "background": "#2377d9",
          "color": "#ffffff",
          "corner_radius": "5",
          "size": "18"
        },
        "btn_done_class": {
          "align": "center",
          "height": "60",
          "font": "HelveticaNeue-Bold",
          "border_width": "1",
          "background": "#dee3ea",
          "color": "#000000",
          "corner_radius": "5",
          "size": "18"
        }
      },
      "templates": {
        "body": {
          "header": {
            "title": "User Search",
            "search": {
              "name": "query_text",
              "placeholder": "Search name"
            },
            "style": {
              "theme": "dark"
            }
          },
          "sections": [
            {
              "items": {
                "{{#each users}}": {
                  "type": "horizontal",
                  "components": [
                    {
                      "type": "label",
                      "class": "input_class",
                      "text": "{{name}}\n{{email}}"
                    },
                    {
                      "type": "label",
                      "text": ""
                    },
                    {
                      "type": "button",
                      "class": "btn_ready_class",
                      "text": "+ Add to class",
                      "action": {
                        "type": "$set",
                        "options": {
                          "user_id": "{{id}}"
                        },
                        "success": {
                          "trigger": "check_in"
                        }
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      },
