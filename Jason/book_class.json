{
    "$jason": {
        "head": {
            "title": "demo",
            "styles": {
                "@": "https://json.trufitportal.com/Jason/common_styles.json"
            },
            "actions": {
                "$show": {
                    "trigger": "fetch"
                },
                "$load": {
                    "type": "$network.request",
                    "options": {
                        "url": "https://www.trufitportal.com/{{$global.studio.key}}/api/get_week.json"
                    },
                    "success": {
                        "type": "$set",
                        "options": {
                            "week": "{{$jason.week}}"
                        },
                        "success": {
                            "trigger": "fetch"
                        }
                    }
                },
                "$pull": {
                    "trigger": "fetch"
                },
                "fetch": {
                    "type": "$network.request",
                    "options": {
                        "url": "https://www.trufitportal.com/{{$global.studio.key}}/api/get_classes.json?SortBy=time&week={{$get.week}}&token={{$global.user.token}}"
                    },
                    "success": {
                        "type": "$render"
                    }
                }
            },
            "templates": {
                "body": {
                    "background": "#333333",
                    "header": {
                        "title": "Book Classes",
                        "style": {
                            "background": "#222222",
                            "color": "#ffffff"
                        },
                        "menu": {
                            "text": "Logout",
                            "action": {
                                "options": {
                                    "type": "$global.reset",
                                    "items": [
                                        "token"
                                    ]
                                },
                                "success": {
                                    "type": "$href",
                                    "options": {
                                        "url": "https://jasonbase.com/things/BqG1",
                                        "transition": "replace",
                                        "preload": {
                                            "@": "file://preload.json"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "footer": {
                        "@": "https://json.trufitportal.com/Jason/footer.json"
                    },
                    "layers": [
                        {
                            "type": "image",
                            "url": "https://png.icons8.com/metro/100/ffffff/chevron-right.png",
                            "style": {
                                "top": "90",
                                "right": "10",
                                "width": "50",
                                "padding": "10",
                                "color": "#00FFFF"
                            },
                            "action": {
                                "type": "$set",
                                "options": {
                                    "week": "{{$get.week + 1}}"
                                },
                                "success": {
                                    "trigger": "fetch"
                                }
                            }
                        },
                        {
                            "type": "image",
                            "url": "https://png.icons8.com/metro/100/ffffff/chevron-left.png",
                            "style": {
                                "top": "90",
                                "left": "10",
                                "width": "50",
                                "padding": "10",
                                "color": "#00FFFF"
                            },
                            "action": {
                                "type": "$set",
                                "options": {
                                    "week": "{{$get.week - 1}}"
                                },
                                "success": {
                                    "trigger": "fetch"
                                }
                            }
                        }
                    ],
                    "sections": [
                    {
                        "items": [
                            {
                                "type": "space",
                                "style": {
                                    "height": "50",
                                }
                            },
                            {
                            "type": "label",
                            "text": "{{$jason.data.classes.Monday.date}}",
                            "class": "label_header"
                        }]
                    },
                    {
                        "items": {
                            "{{#each $jason.data.classes.Monday.classes}}": {
                                "type": "horizontal",
                                "style": {
                                    "distribution": "equalsize",
                                    "spacing": "25"
                                },
                                "components": [{
                                    "type": "vertical",
                                    "components": [{
                                        "type": "label",
                                        "text": "{{mStart}} to {{mEnd}}",
                                        "class": "row_normal"
                                    },
                                    {
                                        "type": "label",
                                        "text": "{{ClassType}}",
                                        "class": "row_bold"
                                    },
                                    {
                                        "type": "label",
                                        "text": [{
                                            "{{#if (CancelledDescription != '')}}": "{{CancelledDescription}}"
                                        },
                                        {
                                            "{{#else}}": "{{Teacher || 'No teacher assigned'}}"
                                        }
                                        ],
                                        "class": "row_normal"
                                    }
                                    ]
                                },
                                {
                                    "type": "button",
                                    "text": [{
                                        "{{#if (BookingStatus == 'ok')}}": "book"
                                    },
                                    {
                                        "{{#elseif (BookingStatus == 'full')}}": "waitlist"
                                    },
                                    {
                                        "{{#else}}": "{{BookingStatus}}"
                                    }
                                    ],
                                    "class": [{
                                        "{{#if (BookingStatus == 'ok')}}": "button_ok btn_row"
                                    },
                                    {
                                        "{{#elseif (BookingStatus == 'full')}}": "button_wl btn_row"
                                    },
                                    {
                                        "{{#else}}": "button_def btn_row"
                                    }
                                    ],
                                    "action": [{
                                        "{{#if (BookingStatus == 'ok' || BookingStatus == 'full')}}": {
                                            "type": "$href",
                                            "options": {
                                                "url": "{{mobileLink}}",
                                                "preload": {
                                                    "@": "file://preload.json"
                                                },
                                                "transition": "modal",
                                                "options": {
                                                    "clsID": "{{ClassesID}}",
                                                    "date": "{{Dateformatted}}"
                                                }
                                            }
                                        }
                                    },
                                    {
                                        "{{#else}}": {}
                                    }
                                    ]
                                }
                                ]
                            }
                        }
