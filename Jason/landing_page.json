{
  "$jason": {
    "head": {
      "title": "landing page",
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://www.trufitportal.com/{{$global.location_key}}/api/usercheck.json?token={{$global.token || ''}}"
          },
          "success": {
            "type": "$href",
            "options": {
              "url": [
                {
                  "{{#if $jason.start == 'selfcheckin'}}": "https://jasonbase.com/things/r4AM"
                },
                {
                  "{{#else}}": "https://json.trufitportal.com/Jason/homepage.json"
                }
              ],
              "transition": "replace",
              "preload": {
                "background": "#ffffff",
                "layers": [
                  {
                    "type": "image",
                    "url": "https://json.trufitportal.com/images/kettle_512.png",
                    "style": {
                      "width": "200",
                      "top": "50%-100",
                      "left": "50%-100"
                    }
                  }
                ]
              }
            }
          },
          "error": {
            "type": "$href",
            "options": {
              "url": "https://json.trufitportal.com/Jason/login.json",
              "transition": "replace",
              "preload": {
                "background": "#ffffff",
                "layers": [
                  {
                    "type": "image",
                    "url": "https://json.trufitportal.com/images/kettle_512.png",
                    "style": {
                      "width": "200",
                      "top": "50%-100",
                      "left": "50%-100"
                    }
                  }
                ]
              }
            }
          }
        }
      }
    }
  }
}
