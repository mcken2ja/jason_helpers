{
  "$jason": {
    "head": {
      "title": "landing page",
      "actions": {
        "$load": {
          "type": "$network.request",
          "options": {
            "url": "https://www.trufitportal.com/{{$global.location_key}}/api/usercheck.json?token={{$global.token}}"
          },
          "success": {
            "type": "$href",
            "options": {
              "url": "https://jasonbase.com/things/Kjnz",
              "transition": "replace",
              "preload": {
                "background": "#ffffff",
                "layers": [
                  {
                    "type": "image",
                    "url": "https://i.imgur.com/tao8mUD.png",
                    "style": {
                      "width": "200",
                      "top": "50%-100",
                      "left": "50%-100"
                    }
                  }
                ]
              }
            }
          },
          "error": {
            "type": "$href",
            "options": {
              "url": "https://jasonbase.com/things/BqG1",
              "transition": "replace",
              "preload": {
                "background": "#ffffff",
                "layers": [
                  {
                    "type": "image",
                    "url": "https://i.imgur.com/tao8mUD.png",
                    "style": {
                      "width": "200",
                      "top": "50%-100",
                      "left": "50%-100"
                    }
                  }
                ]
              }
            }
          }
        }
      }
    }
  }
}
